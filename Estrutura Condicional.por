programa {
  funcao inicio() {
  // Estrutura condicional 
  // Declaração de variáveis.
  inteiro valor

  // Solicitando dados.
  escreva("Digite um número: ")
  leia(valor) 


  // 
  se (valor > 10){
    escreva("É maior que 10 ")
  } 
  se(valor < 10){
    escreva("Não é maior que 10 ")}

   se (valor == 10){
    escreva("O número é igual a 10")
   }
  
  }
  }
