programa {
  funcao inicio() {
  // Declaração de variaveis.
  inteiro valor
  

  // Solicitando dados.
  escreva ("Digite o valor: ")
  leia(valor)
  // Resolvendo variaveis.
  se (valor >10) {
    escreva("É Maior que 10")
  } senao{
    escreva("Não é maior que 10")
  }
  escreva("\n=== Fim do programa ===")
  }
}
